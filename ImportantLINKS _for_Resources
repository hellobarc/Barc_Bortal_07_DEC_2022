  handleSelect(myitem){
            let obj = this.multiple_items;
            Object.keys(obj).forEach(key => {
                if(key == myitem){
                     obj[key] = true;
                     this.selected_ans = myitem;
                }else{
                     obj[key] = false;
                     this.selected_ans = false;
                }
            });
        },
